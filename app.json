{
  "expo": {
    "name": "인동장씨무안파통덕랑익호(서홍)제가계도",
    "slug": "family-tree",
    "icon": "./assets/icon.png",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#008ff8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "sourceExts": ["ts", "tsx", "js"],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    },
    "android": {
      "package": "com.jang.familytree",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-fg.png",
        "backgroundColor": "#008ff8"
      }
    }
  }
}
